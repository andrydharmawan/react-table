{
    "name": "@bgscore/react-table",
    "version": "1.0.19",
    "description": "A React utility library that provides advanced API hooks (fetch, mutation, caching) and a customizable Table component with built-in pagination, sorting, and loading states. Built for scalable and flexible data handling in modern React applications.",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrydharmawan/react-table.git"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch"
    },
    "keywords": [],
    "author": "Andry Bagus Dharmawan",
    "license": "MIT",
    "peerDependencies": {
        "@bgscore/react-core": ">=1.0.30",
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.1",
        "@types/react-dom": "^19.1.2",
        "tsup": "^8.4.0",
        "typescript": "^5.8.3"
    }
}